#program to test loops
start;
var x;
var y;
var k;
var j;
var i;
var l;
var m;

x = 1;
y = 3;

for i inrange(x:y): show i endshow endfor;
for j inrange(1:3): show j endshow endfor;
for k inrange(x:3): show k endshow endfor;
for l inrange(1:y): show l endshow endfor;

for(m = y; m >= x; m = m - 1): show m endshow endfor;
for(n = 3; n >= 1; n = n - 1): show n endshow endfor;

end;